<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            default="false"
            id="realignment.editor.jd.ide.eclipse"
            class="jd.ide.eclipse.realignment.editors.RealignmentJDClassFileEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="class"
            name="Realignment for JD Class File Editor">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.jdt.ui.preferences.JavaBasePreferencePage/jd.ide.eclipse.preferences.PreferencePage"
            class="jd.ide.eclipse.realignment.preferences.RealignmentPreferencePage"
            id="realignment.preferences.jd.ide.eclipse"
            name="Batch Realignment">
      </page>
   </extension>



</fragment>
