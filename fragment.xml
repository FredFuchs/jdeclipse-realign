<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="jd.ide.eclipse.realignment.editors.RealignmentJDClassFileEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            default="false"
            extensions="class"
            icon="icons/icon.png"
            id="realignment.editor.jd.ide.eclipse"
            name="Class File Viewer [Decompiled]">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.startup">
      <startup class="jd.ide.eclipse.realignment.Startup"/>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            id="realignment.jd.ide.eclipse.ClassActions"
            objectClass="org.eclipse.jdt.core.IClassFile">
         <menu
               id="realignment.jd.ide.eclipse.menu1"
               label="Open With"
               path="group.navigate">
         </menu>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            class="jd.ide.eclipse.realignment.menu.OpenClassWith"
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer?before=group.edit">
      </menuContribution>
   </extension>

</fragment>
